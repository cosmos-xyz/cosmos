{
  "name": "@cosmos-xyz/cosmos",
  "version": "0.2.0",
  "description": "Feature-rich and extendable revolt.js framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:check": "tsc --noEmit --incremental false",
    "docs": "typedoc --options typedoc.json src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts --fix",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmos-xyz/cosmos.git"
  },
  "keywords": [
    "revolt-chat"
  ],
  "author": "Debert Jamie <debert123@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cosmos-xyz/cosmos/issues"
  },
  "homepage": "https://cosmos.js.org",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.7",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^28.0.0",
    "jsdoc-babel": "^0.5.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "node-fetch": "^3.1.0",
    "revolt-api": "^0.5.3-alpha.12",
    "revolt.js": "^5.2.8"
  },
  "directories": {
    "doc": "docs"
  }
}
